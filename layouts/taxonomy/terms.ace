= content main
  {{ .Content }}
  ul
    {{ $type := .Type }}
    {{ range $name, $pages := .Data.Terms.ByCount }}
      {{ $name := .Name }}
      {{ $count := .Count }}
      {{ with site.GetPage (printf "/%s/%s" $type ($name | urlize)) }}
        li
          a href={{ .RelPermalink }} {{ $name }} ({{ $count }})
      {{ end }}
    {{ end }}
